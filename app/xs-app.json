{
  "welcomeFile": "masterapp/webapp/",
  "authenticationMethod": "route",
  "sessionTimeout": 100,
  "pluginMetadataEndpoint": "/metadata",
  "routes": [
    {
      "source": "^/masterapp/webapp/(.*)",
      "target": "$1",
      "localDir": "masterapp/webapp/",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    }
  ]
}
